<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.solmix.org/schema/rt"
	xmlns:wmix="http://www.solmix.org/schema/wmix-components"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd
           http://www.solmix.org/schema/wmix-components
           http://www.solmix.org/schema/wmix-components/wmix-components.xsd
           http://www.solmix.org/schema/rt 
           http://www.solmix.org/schema/rt/rt.xsd">
	<c:container production="false" />
	<components autoDiscovery="true" defaultComponent="app3" xmlns="http://www.solmix.org/schema/wmix-components">
		<component name="app3" path="/app3">
			<endpoints>
				<bean class="org.solmix.wmix.config.MockEndpoint"  xmlns="http://www.springframework.org/schema/beans"></bean>
			</endpoints>
		</component>
	</components>

</beans>